{
  "dashboard": {
    "title": "Gemini Flow Backend Monitoring",
    "tags": ["gemini-flow", "backend", "prometheus"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Request Throughput (req/sec)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(gemini_flow_http_requests_total[1m])",
            "legendFormat": "{{method}} {{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Requests/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 2,
        "title": "Request Latency Percentiles",
        "type": "graph",
        "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(gemini_flow_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(gemini_flow_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gemini_flow_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Latency" },
          { "format": "short" }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate (%)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "100 * rate(gemini_flow_errors_total[5m]) / rate(gemini_flow_http_requests_total[5m])",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percent", "label": "Error %" },
          { "format": "short" }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": { "params": [5], "type": "gt" },
              "operator": { "type": "and" },
              "query": { "params": ["A", "5m", "now"] },
              "reducer": { "params": [], "type": "avg" },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "title": "Gemini API Performance",
        "type": "graph",
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(gemini_flow_gemini_api_duration_seconds_sum[5m]) / rate(gemini_flow_gemini_api_duration_seconds_count[5m])",
            "legendFormat": "Avg Duration",
            "refId": "A"
          },
          {
            "expr": "rate(gemini_flow_gemini_api_requests_total{status=\"success\"}[5m])",
            "legendFormat": "Success Rate",
            "refId": "B"
          },
          {
            "expr": "rate(gemini_flow_gemini_api_requests_total{status=\"error\"}[5m])",
            "legendFormat": "Error Rate",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Duration / Rate" },
          { "format": "short" }
        ]
      },
      {
        "id": 5,
        "title": "Flow Execution - Nodes Processed",
        "type": "graph",
        "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(gemini_flow_nodes_processed_sum[5m]) / rate(gemini_flow_nodes_processed_count[5m])",
            "legendFormat": "Avg Nodes per Flow",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Nodes" },
          { "format": "short" }
        ]
      },
      {
        "id": 6,
        "title": "Flow Execution - Edges Processed",
        "type": "graph",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(gemini_flow_edges_processed_sum[5m]) / rate(gemini_flow_edges_processed_count[5m])",
            "legendFormat": "Avg Edges per Flow",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Edges" },
          { "format": "short" }
        ]
      },
      {
        "id": 7,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": { "x": 0, "y": 24, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "rate(gemini_flow_process_cpu_seconds_total[1m]) * 100",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percent", "label": "CPU %" },
          { "format": "short" }
        ]
      },
      {
        "id": 8,
        "title": "Memory Usage (MB)",
        "type": "graph",
        "gridPos": { "x": 8, "y": 24, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "gemini_flow_process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Resident Memory",
            "refId": "A"
          },
          {
            "expr": "gemini_flow_process_heap_bytes / 1024 / 1024",
            "legendFormat": "Heap Memory",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "decmbytes", "label": "Memory MB" },
          { "format": "short" }
        ]
      },
      {
        "id": 9,
        "title": "Event Loop Lag",
        "type": "graph",
        "gridPos": { "x": 16, "y": 24, "w": 8, "h": 8 },
        "targets": [
          {
            "expr": "rate(gemini_flow_nodejs_eventloop_lag_seconds[1m])",
            "legendFormat": "Event Loop Lag",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Lag (seconds)" },
          { "format": "short" }
        ]
      },
      {
        "id": 10,
        "title": "HTTP Requests by Status Code",
        "type": "graph",
        "gridPos": { "x": 0, "y": 32, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(gemini_flow_http_requests_total[1m])",
            "legendFormat": "{{status_code}} - {{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Requests/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 11,
        "title": "Errors by Type",
        "type": "graph",
        "gridPos": { "x": 12, "y": 32, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(gemini_flow_errors_total[5m])",
            "legendFormat": "{{type}} - {{path}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Errors/sec" },
          { "format": "short" }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
